package prs.business;

public class PurchaceRequestLineItem {

	
	private int id;
	private int purchaceRequestId;
	private int productId;
	private int quantity;
	
	public PurchaceRequestLineItem(int id, int purchaceRequestId, int productId, int quantity) {
		super();
		this.id = id;
		this.purchaceRequestId = purchaceRequestId;
		this.productId = productId;
		this.quantity = quantity;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getPurchaceRequestId() {
		return purchaceRequestId;
	}

	public void setPurchaceRequestId(int purchaceRequestId) {
		this.purchaceRequestId = purchaceRequestId;
	}

	public int getProductId() {
		return productId;
	}

	public void setProductId(int productId) {
		this.productId = productId;
	}

	public int getQuantity() {
		return quantity;
	}

	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}
	
	
	
	
}
